  127  # -----------------------------------------------------------------------------
  128  # Start Script for the CATALINA Server
  129  # -----------------------------------------------------------------------------
  130  # Better OS/400 detection: see Bugzilla 31132
  131  os400=false
  132  case "`uname`" in OS400*) os400=true;; esac
  133  # resolve links - $0 may be a softlink
  134  PRG="$0"
  135  while [ -h "$PRG" ] ; do   ls=`ls -ld "$PRG"`;   link=`expr "$ls" : '.*-> \(.*\)$'`;   if expr "$link" : '/.*' > /dev/null; then     PRG="$link";   else     PRG=`dirname "$PRG"`/"$link";   fi; done
  136  PRGDIR=`dirname "$PRG"`
  137  EXECUTABLE=catalina.sh
  138  # Check that target executable exists
  139  if $os400; then   eval; else   if [ ! -x "$PRGDIR"/"$EXECUTABLE" ]; then     echo "Cannot find $PRGDIR/$EXECUTABLE";     echo "The file is absent or does not have execute permission";     echo "This file is needed to run this program";     exit 1;   fi; fi
  140  exec "$PRGDIR"/"$EXECUTABLE" start "$@"
